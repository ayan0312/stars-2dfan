{
  "name": "stars-2dfan",
  "version": "1.0.1",
  "scripts": {
    "start": "ts-node ./lib/cli/command.ts --start",
    "cli": "ts-node ./lib/cli/command.ts",
    "server": "ts-node ./lib/server/app.ts",
    "build": "cross-env NODE_ENV=production tsc",
    "dev": "cross-env NODE_ENV=development ts-node ./lib/main.ts",
    "test": "mocha --require ts-node/register --watch-extensions ts,tsx 'test/**/*.ts'",
    "lint": "npm-run-all lint:*",
    "lint:ts": "tslint \"{lib/**/*,test/**/*}.ts\" -p tsconfig.json",
    "lint:prettier": "prettier \"{lib/**/*,test/**/*}.ts\" --list-different",
    "fix": "npm-run-all fix:*",
    "fix:ts": "npm run lint:ts -- --fix",
    "fix:prettier": "prettier \"{lib/**/*,test/**/*}.ts\" --write",
    "precommit": "lint-staged",
    "install": "node-gyp rebuild",
    "rebuild": "node-gyp rebuild",
    "rebuild:dev": "node-gyp rebuild --debug",
    "clean": "node-gyp clean"
  },
  "lint-staged": {
    "linters": {
      "{src,test}/**/*.{js,ts,tsx}": [
        "prettier",
        "tslint -p tsconfig.json"
      ]
    }
  },
  "private": true,
  "pre-push": [
    "validate"
  ],
  "husky": {
    "hooks": {
      "post-commit": "git reset",
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.1.7",
    "@types/cheerio": "^0.22.13",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.2.1",
    "@types/node": "^12.0.8",
    "@types/request": "^2.48.2",
    "@types/superagent": "^4.1.3",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "delay": "^4.3.0",
    "husky": "2.4.1",
    "lint-staged": "^8.2.0",
    "mocha": "^6.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2",
    "ts-loader": "^6.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.17.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "commander": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "inquirer": "^6.5.0",
    "log4js": "^6.4.0",
    "mongodb": "^3.3.0-beta2",
    "request": "^2.88.0",
    "superagent": "^5.1.0"
  }
}
